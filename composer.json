{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phpstan/phpstan": "^0.9.1",
    "phpstan/phpstan-strict-rules": "^0.9.0"
  },
  "autoload": {
    "psr-4": {
      "Hashing\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "phpstan": "phpstan analyse src tests -l 7 -c phpstan.neon",
    "coverage": [
      "test -d extension=xdebug.so --coverage ./coverage.xml --coverage-src ./src tests/"
    ]
  }
}
